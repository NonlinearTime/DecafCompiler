void main() {
  int a;
  a = 0;
  
  while (a != 10) {
    Print(a, " ");
    a = a + 1;
  }
  Print("\na now = ", a, " which is ");
  if (a % 2 == 0)
	Print("even");
  else
	Print("odd");

  a = 0;
  while (true) break;
  while (a != 10) {
    Print(a, " ");
    a = a + 1;
    if (a == 7)
       Print(a);
    else
      break;
  }

  a = 0;
  while (a != 10) {
    Print(a, " ");
    a = a + 1;
    if (a > 7) 
       if (a == 8) 
          if (a == 9)
             break;
  }
}







